@page
@model IdentityPractice.Pages.HumanResource.IndexModel
@{
    ViewBag.Title = "Human Resource";
}

<h1 class="display-4">Human Resource</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Date</th>
            <td>Temp C</td>
            <td>Temp F</td>
            <td>Summary</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.WeatherForecasts)
        {
            <tr>
                <td>@item.Date</td>
                <td>@item.TemperatureC</td>
                <td>@item.TemperatureF</td>
                <td>@item.Summary</td>
            </tr>
        }
    </tbody>
</table>
